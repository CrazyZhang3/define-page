{
  "name": "@uni-ku/define-page",
  "type": "module",
  "version": "0.1.5",
  "description": "Define uniapp pages.json dynamically.",
  "author": "Edwin Xu <edwinhuish@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/uni-ku/define-page",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-ku/define-page.git"
  },
  "bugs": "https://github.com/uni-ku/define-page/issues",
  "keywords": [
    "uniapp",
    "uni-app",
    "pages.json",
    "macros",
    "definePage"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./index.d.ts",
  "files": [
    "client.d.ts",
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "unbuild",
    "stub": "unbuild --stub",
    "debug": "cross-env NODE_ENV=development unbuild && cd playground && pnpm dev:h5",
    "watch": "tsup --watch",
    "typecheck": "tsc -p . --noEmit",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "test": "vitest"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.7",
    "@babel/generator": "^7.23.6",
    "@babel/types": "^7.24.0",
    "@vue/compiler-sfc": "^3.4.21",
    "ast-kit": "^0.12.1",
    "chokidar": "^3.6.0",
    "debug": "^4.3.4",
    "fast-glob": "^3.3.2",
    "magic-string": "^0.30.8",
    "typescript": "^5.4.5",
    "unconfig": "^0.3.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.0",
    "@antfu/utils": "^0.7.7",
    "@babel/generator": "^7.23.6",
    "@babel/traverse": "^7.26.9",
    "@dcloudio/types": "^3.4.8",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.5",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "@vue-macros/common": "^1.10.1",
    "ast-walker-scope": "^0.6.1",
    "bumpp": "^9.4.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "eslint": "^9.17.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "magic-string": "^0.30.8",
    "rimraf": "^5.0.5",
    "unbuild": "^3.3.1",
    "unconfig": "^0.3.11",
    "unplugin": "^1.10.0",
    "vite": "^5.0.0",
    "vitest": "^1.4.0",
    "vuex": "^4.1.0"
  }
}
